/* miniprogram/pages/vip/vip.wxss */
.list-box {
  display: flex;
  padding: 30rpx;
  justify-content: space-between;
}
.price-box {
  display: flex;
  flex-direction: column;
  align-items: center;
  width: 30%;
  border: 2rpx solid #ddd;
  padding: 30rpx 0;
  border-radius: 12rpx;
}
.price-title {
  font-weight: bold;
  font-size: 32rpx;
  margin-bottom: 10rpx;
}
.price-box.active {
  background: var(--less-bg);
  border: 2rpx solid var(--color);
}
